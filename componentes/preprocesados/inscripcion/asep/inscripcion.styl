.FechaConTituloContenido
	display flex
	justify-content space-between
	padding-top 2em
	@media screen and (max-width: 700px)
		display block
	&-Izquierda
		width 47%
		padding-left 1.5em
		@media screen and (max-width: 700px)
			width 100%
		& img
			width 100%
		&-Texto
			text-align justify
			color #424242
			padding-top 0.5em
	&-Derecha
		width 47%
		background #E0E0E0
		display flex
		justify-content center
		align-items center
		flex-direction column
		color #383838
		@media screen and (max-width: 700px)
			width 100%
		& div
			margin-bottom 0.5em
		&-Reloj
			//margin-top -8em
		&-Reloj i
			font-size 2em
		&-Hora
			font-size 2em
		&-Mapa i
			font-size 2em
		&-Direccion
			font-size 1.4em
			text-align center
			width 60%
		&-Numeros
			font-size 3em
			color #38B34B
		&-CursosDisponibles
			font-weight bold
			font-size 1.3em
			margin-top -1em
		&-InscripcionGratuita
			width 40%
			text-align center
			color #383838
		&-BtnInscribete
			color white
			background #38B34B
			padding 0.4em 2em
			font-size 1.2em
			font-weight bold
			border-radius 6px
			cursor pointer
		&-Formulario
			color #383838
			height 0em
			overflow hidden
			transition all ease 0.5s
			//margin-left -4em
			& td
				color #383838				
			& input
				width 100%
				border none
				height 2em
			&-Enviar
				background #fdaf17
				color white
				width 10em
				padding-top 0.5em
				padding-bottom 0.5em
				border-radius 6px
				margin 0 auto
				display flex
				justify-content center
				align-items center
				font-weight bold
				margin-top 1em
			&-EnviarClass
				display flex
				justify-content center
				align-items center			
		&-BtnHazteSocio
			height 4em
			background url(rutaImagenes+"/btn_hazte_socio_blue.png") no-repeat left
			width 12em
			background-size 12em
			transition all ease 0.5s
			cursor pointer
			&:hover
				height 4em
				background url(rutaImagenes+"/btn_hazte_socio.png") no-repeat left
				width 12em
				background-size 12em
				transition all ease 0.5s
				cursor pointer