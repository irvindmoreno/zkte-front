//************** icons gallito **************//
.post-gallito-title
    position relative
    margin-bottom 1em
.smB
    margin-left 0px
    margin-right 0px
.post-gallito-title-txt
    color: #3b404b;
    font-size: 1.8em;
    font-weight: bold
    display: inline-block;
    background: white;
    padding-right 0.5em
.post-gallito-title-borde
    border-bottom: 1px solid #404040;
    position: absolute;
    top: 28px;
    width: 100%;
    z-index: -1;
.contenedor-gallito-icons
    border-top: 1px solid gray;
    padding: 1em 0em;
.contenedor-area-interes-icons
    background: #efefef;
    padding: 1em;
    text-align center
.area-interes-icon-cuadro
    display: inline-block;
    width: 13%
    cursor pointer
    transition all ease 0.3s
    vertical-align top
    text-align center
    padding 1.4em 0em
    min-width 11em
.area-interes-icon-cuadro:hover
    background #e3e3e5
.contenedor-img-icon-area-interes
    text-align: center;
    margin 0 auto
.contenedor-img-icon-area-interes img
    height: 3.6em;
    margin: 0 auto;
.span-icon-area-interes
    display: inline-block;
    text-align: center;
    color: #343d49;
    font-weight: bold;
    padding: 1em 0em 0em 0em;
    font-size 14px
.icon-pad-1
    padding: 1em 2em 1em 2em;
@media screen and (max-width: 600px)   
    .area-interes-icon-cuadro
        min-width 9em
