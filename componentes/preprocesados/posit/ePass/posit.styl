//************** posit ePass **************//
colorOldebrechRed=#E0333D
.hola
	height 50em
.posit-Epass-Container
	position fixed
	background white
	right 0
	top 25%
.posit-Epass-Consulta-Oculta
	background colorOldebrechRed
	color white
	display flex
	justify-content center
	align-items center
	flex-direction column
	width 2em
	cursor pointer
	float right
	height 13.5em
	font-weight bold
	font-size 1.4em
	line-height 1.23em
	border 1px dashed #D4D4D4
	border-radius 0.2em
.posit-Epass-Consulta-Oculta
	display none
.posit-Epass
	width 8em
.posit-Epass-Titulo,.posit-Epass-Footer
	background colorOldebrechRed
	color white
	text-align center
	height 2em
	display flex
	justify-content center
	align-items center
	font-weight bold
	cursor pointer
.posit-Epass-MisSaldos
	height 7em
	color colorOldebrechRed
	display flex
	flex-direction column
	justify-content center
	align-items center
	border-left 1px dashed #757873
	border-right 1px dashed #757873
	border-top 1px dashed #757873
.posit-Epass-titulo
	font-size 1.1em
.ePass-flecha
	font-size 2em
.fa-refresh,.fa-hand-o-up,.fa-credit-card
	font-size 1.5em
.posit-Epass-MisSaldos-Contenido
	display flex
	justify-content space-between
	align-items center
	width 83%
.fa-angle-right,.fa-angle-left
	font-size 1.5em
.letras-90deg
	transform rotate(-90deg)
	-webkit-transform rotate(-90deg)
	-moz-transform rotate(-90deg)
	-o-transform rotate(-90deg)
//************** posit ePass **************//
