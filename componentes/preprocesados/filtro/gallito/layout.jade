- rutaStatic ='../../../../static'
doctype html
html
    head
        meta(charset='utf-8')
        meta(name='viewport' content='width=device-width, user-scalable=no')
        block title
            title gallito-filtro
        block link
            link(rel='stylesheet' href=rutaStatic+'/css/bootstrap.min.css')
            link(rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.6.3/css/font-awesome.min.css')
            link(rel='stylesheet' type='text/css' href='./filtro.css')
            link(rel='stylesheet' href=rutaStatic+'/imagenes/sprite/gallito/sprite.css')

    body
        block content
        block script
            script(src=rutaStatic+'/js/jquery.min.js')
            script(src=rutaStatic+'/js/bootstrap.min.js')
            script(src='./filtro.js')
