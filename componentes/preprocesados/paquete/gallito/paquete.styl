.triangle-left
    position absolute
    transform rotate(-180deg)
    bottom -0.28em
    width: 0;
    height: 0;
    border-right: 9px solid transparent;
    border-top: 9px solid transparent;
    border-left: 9px solid #3a404b;
    border-bottom: 9px solid #3a404b;
    z-index -5
.triangle-right
    position absolute
    transform rotate(90deg)
    bottom -0.28em
    width: 0;
    height: 0;
    right: 0
    border-right: 9px solid transparent;
    border-top: 9px solid transparent;
    border-left: 9px solid #3a404b;
    border-bottom: 9px solid #3a404b;
    z-index -5
