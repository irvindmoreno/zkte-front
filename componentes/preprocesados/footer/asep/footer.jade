// Footer Asep
extends ./layout.jade
block content
    - rutaImagenes ='../../../imagenes'
    - colaborador1="http://asep.pe/wp-content/themes/asep/img/inkuba.png"
    - colaborador2="http://asep.pe/wp-content/themes/asep/img/clicksandbricks.png"
    - colaborador3="http://asep.pe/wp-content/themes/asep/img/labase.png"
    - colaborador4="http://asep.pe/wp-content/themes/asep/img/mujeres.png"
    - sponsor1="http://asep.pe/wp-content/themes/asep/img/bid.png"
    - logoAsep="http://asep.pe/wp-content/themes/asep/img/asep_footer.png"
    - logoFotterFin1="sombrero.png"
    footer(class="Fotter")
        div(class="Fotter-1")
            div(class="Fotter-1-btn-HazteSocio")
        div(class="Fotter-2")
            div(class="Fotter-2-Colaboradores")
                span Colaboradores
                div(class="Fotter-2-Colaboradores-Img")
                    img(src=colaborador1)
                    img(src=colaborador2)
                    img(src=colaborador3)
                    img(src=colaborador4)
            div(class="Fotter-2-Sponsor")
                span Sponsor
                div(class="Fotter-2-Sponsor-Img")
                    img(src=sponsor1)
        div(class="Fotter-3")
            div
                img(src=logoAsep)
            div
                nav(class="Fotter-3-Navegacion-Menu")
                    ul
                        li(class="li-active")
                            a(class="active") Nosotros
                        li
                            a(class="inactive") Directorio
                        li
                            a(class="inactive") Actividades
                        li
                            a(class="inactive") Noticias
                        li
                            a(class="inactive") Beneficios
                        li
                            a(class="inactive") Preg√∫ntanos
                div(class="Fotter-3-Letter")
                    span(class="Fotter-3-Letter-Span").
                        Miguel Dasso 153 - piso A - San Isidro / info@asep.pe 
                    div(class="Emprendedor-RedesSociales")
                        span(class="facebook")
                            i(class="fa fa-facebook-official")
                        span(class="twiter")
                            i(class="fa fa-twitter-square")
                        span(class="linkedin")
                            i(class="fa fa-linkedin-square")
        div(class="Fotter-4")
            span Powered by La Base
            img(src=logoFotterFin1)

// Footer Asep