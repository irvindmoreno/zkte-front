	footer
		div(class="row footer-gallit-block-1 smb")
			div(class="col-xs-6 col-sm-6 col-md-4 bloq-foot bloq-foot-left pad-mob-foot")
				div(class="footer-gallito-title") Trabajo.gallito.com.uy
				div(class="leter-footer-gallito") Preguntas Frecuentes
				div(class="leter-footer-gallito") Términos y Condiciones
				div(class="leter-footer-gallito") &nbsp;
			div(class="col-xs-6 col-sm-6 col-md-2 bloq-foot pad-mob-foot")
				div(class="footer-gallito-title") Postulantes
				div(class="leter-footer-gallito") Busca Trabajo
				div(class="leter-footer-gallito") Crea tu cuenta
				div(class="leter-footer-gallito") Ingresa
			div(class="col-xs-6 col-sm-6 col-md-2 bloq-foot bloq-foot-3-1 pad-mob-foot")
				div(class="footer-gallito-title") Empresa
				div(class="leter-footer-gallito") Publica tu aviso
				div(class="leter-footer-gallito") Crea tu cuenta
				div(class="leter-footer-gallito") Ingresa
			div(class="col-xs-6 col-sm-6 col-md-4 bloq-foot pad-mob-foot")
