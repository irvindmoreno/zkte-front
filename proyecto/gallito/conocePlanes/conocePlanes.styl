rutaComponentes='../../../componentes'
rutaImagenes='../../../imagenes'


/************Layout****************/
@import('../layout/layout.styl')
/************Layout****************/
.text-2-line-height
    display inline-block
    width 10em
.text-2-line-height
    line-height 1
.contenido-banner-registro_aviso
    height 9em
.contenido-banner-registro_aviso-text
    height 4.5em
/************** formulario gallitoRegistro **************/
body
    margin 0
    font-size 16px
blueDark=#353e4a
blueLight=#527494
redError=#ad3a3a
colorLetterForm=#353e4a
colorError=#ad3a3a
.icon-inline
    display inline-block
    vertical-align top
.smb
    margin-left 0px
    margin-right 0px
.spb
    padding-right 0px
    padding-left 0px
.contenedor-title-borde
    position relative
    font-family calibri
.title-text-borde
    color white
    font-size 1.3em
    padding 0.2em 0.8em
    display inline-block
.borde-inferior-titulo
    background blueLight
    height 0.05em
    width 100%
    position absolute
    bottom 8px;
    z-index -1
.label-form-gallito
    font-weight normal
    cursor pointer
.label-form-gallito,#check_no_tengo_estudios
    display inline-block
    vertical-align middle
    margin-bottom 0px
#check_no_tengo_estudios
    margin-top 1px
    margin-left 0.5em
.cont-file
    padding 0.5em 0em
    position relative
.contenedor-formulario-gallito
    font-family calibri
.input_form_perfil
    width 100%
    outline none
    padding 0.4em 0.8em
    border 1px solid gray
    background white
.align-middle-text
    margin-top 0.5em
.error-format
    color redError
    margin-left 0.2em
.icon-alert-left
    margin-left 0.5em
.cont-file
    color colorLetterForm
.text-2-line
    width 9em
    display inline-block
    line-height 1
.text-2-line-error
    display inline-block
    min-width 6em
.select_color
    color gray
    cursor pointer
.slecet-izquieda
    padding-right 1em
.slecet-derecha
    padding-left 1em
.pad-last-check
    padding-top 0em
.btn-form-gallito
    border none
    padding 0.4em 3em
    color white
    font-weight bold
    cursor pointer
.btn-gallit-ligth
    background blueLight
    margin-bottom 1em
    display inline-block
.btn-gallit-green
    background #0cb570
.content-btn-form
    text-align right
    padding-right 3em
.textarea-form-gallito
    resize none
    height 10em
.subtitle-form-gallito-registro
    color #4d8ac8
    font-weight bold
    margin-bottom 0.3em
    font-size 1.2em
.cuadro-item
    width 100%
.cuadro-item-title
    display: inline-block;
    background: #4d8ac8;
    color: white;
    font-family scfly
    font-size 1.1em
    position relative
    margin-right 0.5em
    margin-top 0.5em
    width 100%
    padding 0.3em 1em
    cursor pointer
.contenedor-add-block
    padding 0em 4%
.cuadro-item-cont-desplegado
    border 1px solid rgba(53, 62, 74, 0.61)
    background #e6e7e8
.style-close
    position: absolute;
    right: 7px;
    top: 8px;
    font-size: 1em;
    cursor pointer
.style-icon-cuadro-item,.style-save
    position: absolute;
    right: 25px;
    top: 9px;
    font-size: 1em;
    cursor pointer
.Text-Pregunta
    text-align  center
.contenedor-Btn-edit
    text-align right
.contenedor-Btn-edit i
    margin-right 0.2em
    cursor pointer
.contenedor-pregunta
    background #4d8ac8
    color white
    padding 0.8em 0.5em
    border-radius 5px
    border 1px solid blueLight
    margin-bottom 0.3em
#archivo
    width 100%
.text-saldo-preguntas,.text-sin-preguntas
    text-align right
    color gray
.cuatro-item-created
    display none
.btn-atras-gallito
    background blueLight
    color white
.icon-up-filter,.icon-down-filter
    font-size: 2em;
    position: absolute;
    right: 0.5em;
.block-desplegable-rectangle
    cursor pointer
    position relative
.contenedor-icon-gallito-perfil
    border-bottom 1px solid white
@media screen and (max-width: 768px)
    .text-2-line
        width 100%
    .slecet-derecha
        margin-top 0.5em
        padding-left 0em
    .slecet-izquieda
        padding-right 0em
    .cont-module-error
        margin-top 0em
    .icon-alert-left
        margin-left 0em
    .align-middle-text
        margin-top 0em
    .contenedor-paginacion
        padding 0em 1em
@media screen and (max-width: 573px)
    .content-btn-form
        text-align center
    #btn-guardarDatos-finta,#addEstudio
        width 100%
        display inline-block
        text-align center
    .btn-form-gallito
        margin-left 0em
    .content-btn-form
        padding-right 0em
/************** formulario gallito **************/

.borde-azul-todobusco  
    border: 6px solid #0e5682;
.br-slider-azul
    border-right 3px solid #0e5682;
.borde-green-todobusco  
    border: 6px solid #b9bd33;
.br-slider-green
    border-right 3px solid #b9bd33;
.contenedor-icon-gallito-perfil
    background blueDark
    color white
.color-error
    color colorError
.contenedor-datos-cargo
    padding 1em 4%
.contenedor-paginacion
    padding 0em 4%
.text-1-line
    margin-top: 0.5em;
    display: inline-block;
#image_upload_preview
    width 100%
.text-img-warning
    color #6d6e70
    font-size 0.9em
.span-text-warning
    margin-left 0.5em
.contenedor-img-upload
    background #e6e7e8
    border 1px solid #696969
    height 10em
    width 15em
    overflow hidden
    display inline-block
    margin-top 0em
    text-align center
.center
    text-align center
.text-gallito-img-upload
    font-family: calisto;
    font-size: 2em;
    padding-top: 1.6em;
    display: inline-block;
.hideDiv
    display none
.align-right
    text-align right
.error-validacion
    border 1px solid #ad3a3a
.contenedor-tab-gallito
    margin-top 1em
.input-documento
    margin-top 0.5em
label.error
    font-weight normal
@media screen and (max-width: 802px)
    .contenedor-datos-cargo,.contenedor-paginacion
        padding 1em 4%
    .contenedor-paginacion
        padding-top 0em
        margin-top -0.5em
        padding-bottom 1em
@media screen and (max-width: 600px)    
    .imagen-slider-empresa
        height 100%    
    .cuadro-todobusco-slider-casabusco
        background #efb762
    .borde-azul-todobusco
        background #337dab
    .borde-green-todobusco
        background #ccd06b
    .borderojo-todobusco
        background rgb(216, 163, 163)
@media screen and (max-width: 542px)
    .post-gallito-title-txt
        font-size 1.1em
        text-align center
    .post-gallito-title
        margin-bottom 0em
    .contenedor-post-gallito
        border-top: 1px solid #808080;
        padding: 1em 0em 0em 0em
    .btn-form-gallito
        width 100%
        padding 0.5em 0em
        text-align center
    .content-img-upload
        width 100%
        text-align center
@media screen and (max-width: 496px)
    .contenedor-datos-cargo,.contenedor-paginacion
        padding 1em 1em
    .contenedor-paginacion
        margin-top -2.5em
    .btn-form-gallito
        margin-top 1em
@media screen and (max-width: 390px)
    .imagen-slider-empresa
        height initial
//************** formulario gallitoRegistro **************//
/********************otros*******************/
#formulario-registro-1
    margin-top 1em
@media screen and (max-width: 768px)
    .contenedor-vista-paso-1
        padding 1em 0em
/********************otros*******************/
.title-text-borde
    color white !important
    background inherit !important
.block-desplegable-rectangle
    padding 0em
.icon-check-green
    color: #0cb570;
    font-size: 2em;
    display: inline-block;
    vertical-align: middle;
    margin-right: 0.5em;
.text_check
    font-size: 1em;
    font-weight: bold;
    color: #353e4a;
.contenedor-check-text
    margin-bottom 1em
.contenedor-formulario-gallito
    margin-top 2em