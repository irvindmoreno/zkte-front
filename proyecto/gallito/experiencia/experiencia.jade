extends ../layout/layout.jade
block head
    link(href='./experiencia.css' rel='stylesheet')
block navegacion    
block contenido
    div(class="row smb")
        div(class="col-sm-3 col-md-3 spb contenedor-project-filter")
            include ../include/dashboard/filtro.jade
        div(class="col-sm-9 col-md-9 contenedor-filtro-proyecto")
            - vistaDashboard="Mi Perfil"
            include ../include/dashboard/menu_icons.jade
            //************** formulario gallito **************//
            div(class="contenedor-slider-todobusco")
                form(class="formulario-gallito-slider" method="post" action="")
                    div(id="slider_3_demo")
                        div(class="row smb")
                            div(class="span12")
                                div(id="owl-slider_3" class="owl-carousel")
                                    div(class="item")            
                                        div(class="contenedor-formulario-gallito")
                                            div(class="contenedor-icon-gallito-perfil")
                                                div(class="contenedor-title-borde")
                                                    span(class="title-text-borde-2") Experiencia(1)
                                                    div(class="borde-inferior-titulo")
                                                div(class="row smb cont-file")
                                                    div(class="col-md-12 spb")
                                                        label(for="check_no_tengo_estudios" class="label-form-gallito") No tengo Experiencia
                                                        input(type="checkbox" id="check_no_tengo_estudios")
                                                div(class="row smb cont-file")
                                                    div(class="col-sm-2 col-md-2 spb")
                                                        span(class="text-2-line") Nombre de la empresa (*)
                                                    div(class="col-sm-7 col-md-7 spb align-middle cont-msj-relative")
                                                        input(maxlength="100" type="text" class="input_form_perfil" name="nombre_empresa" placeholder="Ingrese nombre de la institución" id="id_nombre_empresa")
                                                    div(class="col-sm-3 col-md-3 spb align-middle-text cont-module-error")
                                                        div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                        div(class="error-format text-2-line-error")
                                                div(class="row smb cont-file")
                                                    div(class="col-sm-2 col-md-2 spb align-middle-text")
                                                        span(class="text-2-line ") Rubro (*)
                                                    div(class="col-sm-7 col-md-7 spb align-middle cont-msj-relative")
                                                        select(class="input_form_perfil select_color" name="name_rubro" id="id_rubro")
                                                            option(value="") Seleccione
                                                            option(value="1") Informática
                                                            option(value="2") Sistemas
                                                            option(value="3") Idiomas
                                                    div(class="col-sm-3 col-md-3 spb align-middle-text cont-module-error")
                                                        div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                        div(class="error-format text-2-line-error")
                                                div(class="row smb cont-file")
                                                    div(class="col-sm-2 col-md-2 spb")
                                                        span(class="text-1-line") Nivel del cargo (*)
                                                    div(class="col-sm-7 col-md-7 spb cont-msj-relative")
                                                        select(class="input_form_perfil select_color" name="select_tipo_carrera")
                                                            option(value="") Seleccione
                                                            option(value="1") Informática
                                                            option(value="2") Sistemas
                                                            option(value="3") Idiomas
                                                    div(class="col-sm-3 col-md-3 spb align-middle-text cont-module-error")
                                                        div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                        div(class="error-format text-2-line-error")
                                                div(class="row smb cont-file")
                                                    div(class="col-sm-2 col-md-2 spb align-middle-text")
                                                        span(class="") Área (*)
                                                    div(class="col-sm-7 col-md-7 spb cont-msj-relative")
                                                        select(class="input_form_perfil select_color" name="select_nivel")
                                                            option(value="") Seleccione
                                                            option(value="1") Informática
                                                            option(value="2") Sistemas
                                                            option(value="3") Idiomas
                                                    div(class="col-sm-3 col-md-3 spb align-middle-text cont-module-error")
                                                        div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                        div(class="error-format text-2-line-error")
                                                div(class="row smb cont-file")
                                                    div(class="col-sm-2 col-md-2 spb align-middle-text")
                                                        span(class="") Inicio (*)
                                                    div(class="col-sm-7 col-md-7 spb")
                                                        div(class="col-sm-6 col-md-6 spb slecet-izquieda cont-msj-relative")
                                                            select(id="id_select_mes" class="validate-2-select input_form_perfil select_color" name="select_inicio" value="select_two")
                                                                option(value="") Seleccione
                                                                option(value="1") Enero
                                                                option(value="2") Febrero
                                                                option(value="3") Marzo
                                                            div(id="cont-error-mes" class="spb align-middle-text cont-module-error")
                                                                div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                                div(class="error-format text-2-line-error")
                                                        div(class="col-sm-6 col-md-6 spb slecet-derecha cont-msj-relative")
                                                            select(id="id_select_anio" class="validate-2-select input_form_perfil select_color" name="select_mes" value="select_two")
                                                                option(value="") Seleccione
                                                                option(value="1") 2016
                                                                option(value="2") 2015
                                                                option(value="3") 2014
                                                            div(id="cont-error-anio" class="spb align-middle-text cont-module-error")
                                                                div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                                div(class="error-format text-2-line-error")
                                                div(class="row smb cont-file")
                                                    div(class="col-sm-2 col-md-2 spb align-middle-text")
                                                        span(class="") Fin (*)
                                                    div(class="col-sm-7 col-md-7 spb")
                                                        div(class="col-sm-6 col-md-6 spb slecet-izquieda cont-msj-relative")
                                                            select(id="id_select_mes" class="validate-2-select input_form_perfil select_color" name="select_inicio" value="select_two")
                                                                option(value="") Seleccione
                                                                option(value="1") Enero
                                                                option(value="2") Febrero
                                                                option(value="3") Marzo
                                                            div(id="cont-error-mes" class="spb align-middle-text cont-module-error")
                                                                div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                                div(class="error-format text-2-line-error")
                                                        div(class="col-sm-6 col-md-6 spb slecet-derecha cont-msj-relative")
                                                            select(id="id_select_anio" class="validate-2-select input_form_perfil select_color" name="select_mes" value="select_two")
                                                                option(value="") Seleccione
                                                                option(value="1") 2016
                                                                option(value="2") 2015
                                                                option(value="3") 2014
                                                            div(id="cont-error-anio" class="spb align-middle-text cont-module-error")
                                                                div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                                div(class="error-format text-2-line-error")
                                                div(class="row smb cont-file")
                                                    div(class="col-sm-2 col-md-2 spb")
                                                        span(class="text-2-line") Descripción de tareas (*)
                                                    div(class="col-sm-7 col-md-7 spb align-middle cont-msj-relative")
                                                        textarea(class="input_form_perfil" name="nombreInstitucional" placeholder="Ingrese nombre de la institución")
                                                    div(class="col-sm-3 col-md-3 spb align-middle-text cont-module-error")
                                                        div(class="icon-alert icon-inline icon-alert-left hideDiv")
                                                        div(class="error-format text-2-line-error")
                                            div(class="row smb cont-file pad-last-check")
                                            div(class="col-sm-2 col-md-2 spb")
                                            div(class="col-sm-7 col-md-10 spb align-middle")
                                                input(type="checkbox" id="check_estudiando")
                                                label(for="check_estudiando" class="label-form-gallito") Actualmente Tranbajando
                                            div(class="col-sm-3 col-md-3 spb align-middle-text cont-module-error")
                        div(class="row smb")
                            div(class="col-sm-2 col-md-2 spb")
                            div(class="col-sm-7 col-md-7 spb align-middle contenedor-paginacion")
                                div(class="arrow-prev arrow-transale-pagination ")
                                    i(class="fa fa-caret-left disabled-arrow" aria-hidden="true")
                                span(class="content-pagination")
                                    input(type="button" class="pagination-formulario active-pagination " value="1")
                                div(class="arrow-next arrow-transale-pagination ")
                                    i(class="fa fa-caret-right disabled-arrow" aria-hidden="true")
            div(class="content-btn-form")
                button(id="addEstudio" class="btn-form-gallito btn-gallit-ligth" ) Añadir otro estudio
                button(class="btn-form-gallito btn-gallit-green" id="btn-guardarDatos-finta") Guardar cambios
            //************** formulario gallito **************//
block script
    script(src='./clases.js')
    script(src='./experiencia.js')
